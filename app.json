{
  "expo": {
    "name": "grow-habit",
    "slug": "grow-habit",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/growhabitlogo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "scheme": "grow-habit",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.amanfang.growhabit",
      "googleServicesFile": "./GoogleService-Info.plist"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.amanfang.growhabit",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "@react-native-google-signin/google-signin"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "dde47de0-371b-477e-86d1-2f649c7d2179"
      }
    }
  }
}
